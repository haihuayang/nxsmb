#!/bin/sh

# heimdal/configure

PACKAGE_STRING='Heimdal 7.99.1'

true || cat << EOF
#if 1
#ifndef VERSION_HIDDEN
#define VERSION_HIDDEN
#endif
VERSION_HIDDEN const char *heimdal_long_version = "@(#)\$Version: $PACKAGE_STRING by @USER@ on @HOST@ @BRANCH@ @TAG@ ($host) @COMMIT@ @DATE@ \$";
VERSION_HIDDEN const char *heimdal_version = "Heimdal 7.99.1";
#endif
EOF

# ./buildtools/wafsamba/samba_version.py SambaVersion
cat << EOF
/* Autogenerated by waf */
#define SAMBA_VERSION_MAJOR 4
#define SAMBA_VERSION_MINOR 3
#define SAMBA_VERSION_RELEASE 11
#define SAMBA_VERSION_OFFICIAL_STRING "4.3.11"

#ifdef SAMBA_VERSION_VENDOR_FUNCTION
#  define SAMBA_VERSION_STRING SAMBA_VERSION_VENDOR_FUNCTION
#else /* SAMBA_VERSION_VENDOR_FUNCTION */
#  define SAMBA_VERSION_STRING "4.3.11"
#endif
/* Version for mkrelease.sh:
SAMBA_VERSION_STRING=4.3.11
*/
EOF
